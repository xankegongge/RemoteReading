<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!-- 系统名称 -->
    <add key="SoftwareName" value="RemoteReading"/>
    <!--ESFramework服务器监听的端口-->
    <add key="Port" value="4530"/>

    <!--数据库相关信息配置-->
    <!--使用内存虚拟数据库-->
    <add key="UseVirtualDB" value="false"/>
    <!--数据库名称-->
    <add key="DBName" value="RemoteReading"/>
    <!--数据库IP-->
    <add key="DBIP" value="127.0.0.1"/>
    <!--数据库sa的密码-->
    <add key="SaPwd" value="Dl1111@2222"/>
    <add key="sql_connstr" value="Data Source=127.0.0.1;Initial Catalog=RemoteReading;Persist Security Info=True;User ID=sa;Password=Dl1111@2222;"/>
    <!-- ************************* OMCS 配置 ***************************** -->
    <!--OMCS服务器监听的端口-->
    <add key="OmcsPort" value="9900"/>
  </appSettings>
</configuration>